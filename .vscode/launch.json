{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Express Server",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/CalorieCalculator/server.js", // or whatever entry file your Express app has
			"cwd": "${workspaceFolder}",
			"console": "integratedTerminal",
			"env": {
				"NODE_ENV": "development"
			},
			"outFiles": ["${workspaceFolder}/**/*.js"]
		}
	]
}
